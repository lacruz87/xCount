<p id="notice"><%= notice %></p>

<h1>E1ubicacions</h1>

<div class="table-responsive">
<table class="table">
  <thead>
    <tr>
      <th>Nombre</th>
      <th>Almacen</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @e1ubicacions.each do |e1ubicacion| %>
      <tr>
        <td><%= e1ubicacion.nombre %></td>
        <td><%= e1ubicacion.almacen %></td>
        <td><%= link_to 'Show', e1ubicacion %></td>
        <td><%= link_to 'Edit', edit_e1ubicacion_path(e1ubicacion) %></td>
        <td><%= link_to 'Destroy', e1ubicacion, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>
</div>
<br>

<%= link_to 'New E1ubicacion', new_e1ubicacion_path %>
