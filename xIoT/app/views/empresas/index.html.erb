<p id="notice"><%= notice %></p>

<h1>Empresas</h1>

<div class="table-responsive">
<table class="table">
  <thead>
    <tr>
      <th>Nombre empresa</th>
      <th>Rut empresa</th>
      <th>Url app</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @empresas.each do |empresa| %>
      <tr>
        <td><%= empresa.nombre_empresa %></td>
        <td><%= empresa.rut_empresa %></td>
        <td><%= empresa.url_app %></td>
        <td><%= link_to 'Show', empresa %></td>
        <td><%= link_to 'Edit', edit_empresa_path(empresa) %></td>
        <td><%= link_to 'Destroy', empresa, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>
</div>
<br>

<%= link_to 'New Empresa', new_empresa_path %>
